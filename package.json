{
  "name": "@status/defaults",
  "version": "0.0.0-development",
  "description": "Transparently provide default values to objects",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.mjs",
  "types": "dist/types.d.ts",
  "exports": {
    "import": "./dist/module.mjs",
    "require": "./dist/main.js"
  },
  "scripts": {
    "test": "mocha",
    "precompile": "rimraf build",
    "compile": "tsc",
    "build": "rimraf dist && parcel build",
    "watch": "parcel watch",
    "cspell": "cspell *",
    "coverage:check": "nyc check-coverage",
    "precoverage:report": "npm run coverage:test",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:post": "codecov",
    "precoverage:test": "rimraf coverage",
    "coverage:test": "nyc mocha",
    "lint": "eslint ./src --ext .ts",
    "fix": "eslint ./src --ext .ts --fix",
    "nyc": "nyc",
    "precommit": "npm run coverage:test && npm run coverage:check",
    "prepush": "npm run coverage:test",
    "prepack": "npm run build",
    "check": "npm-check -u"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfrazx/defaults.git"
  },
  "keywords": [
    "defaults",
    "proxy",
    "reflect",
    "hash",
    "array",
    "get",
    "set"
  ],
  "author": "jfrazx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfrazx/defaults/issues"
  },
  "homepage": "https://github.com/jfrazx/defaults#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@parcel/packager-ts": "^2.13.2",
    "@parcel/transformer-typescript-types": "^2.13.2",
    "@parcel/watcher": "^2.5.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.1",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "chai": "^4.3.7",
    "codecov": "^3.8.3",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^14.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "mocha": "^10.2.0",
    "npm-check": "^6.0.1",
    "nyc": "^17.1.0",
    "parcel": "^2.13.2",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.0",
    "semantic-release-cli": "^5.4.6",
    "sinon": "^19.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "mocha": {
    "recursive": true,
    "spec": "test/*.spec.ts",
    "reporter": "spec",
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ]
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
