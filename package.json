{
  "name": "@status/defaults",
  "version": "0.0.0-development",
  "description": "Provide default values to objects",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha --require ts-node/register test/*.spec.ts",
    "tsc": "tsc",
    "compile": "tsc -d",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:post": "codecov",
    "coverage:test": "nyc mocha --require source-map-support/register test/*.spec.js",
    "nyc": "nyc",
    "precommit": "npm run coverage:test && npm run coverage:check",
    "prepush": "npm run coverage:test",
    "publish": "npx semantic-release",
    "semantic-release": "semantic-release",
    "semantic:cli": "semantic-release-cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfrazx/defaults.git"
  },
  "keywords": [
    "defaults",
    "proxy",
    "reflect",
    "hash"
  ],
  "author": "jfrazx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfrazx/defaults/issues"
  },
  "homepage": "https://github.com/jfrazx/defaults#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "semantic-release": "^15.13.3",
    "semantic-release-cli": "^4.1.0",
    "source-map-support": "^0.5.10",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
