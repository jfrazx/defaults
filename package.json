{
  "name": "@status/defaults",
  "version": "0.0.0-development",
  "description": "Provide default values to objects",
  "main": "build/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha --require ts-node/register test/*.spec.ts",
    "prebuild": "rimraf build",
    "build": "tsc",
    "precoverage:test": "rimraf coverage",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:post": "codecov",
    "coverage:test": "nyc mocha",
    "nyc": "nyc",
    "precommit": "npm run coverage:test && npm run coverage:check",
    "prepush": "npm run coverage:test",
    "publish": "npx semantic-release",
    "semantic-release": "semantic-release",
    "semantic:cli": "semantic-release-cli",
    "lint": "eslint ./src --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfrazx/defaults.git"
  },
  "keywords": [
    "defaults",
    "proxy",
    "reflect",
    "hash",
    "array",
    "get",
    "set"
  ],
  "author": "jfrazx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfrazx/defaults/issues"
  },
  "homepage": "https://github.com/jfrazx/defaults#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "husky": "^4.3.6",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.3.0",
    "semantic-release-cli": "^5.4.0",
    "source-map-support": "^0.5.19",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "mocha": {
    "recursive": true,
    "spec": "test/*.spec.ts",
    "reporter": "spec",
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ]
  }
}
